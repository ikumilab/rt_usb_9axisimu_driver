<launch>
  <arg name="port" default="/dev/ttyACM0" />
  <arg name="frame_id" default="imu_link" />

  <node pkg="rt_usb_9axisimu_driver" type="rt_usb_9axisimu_driver" name="rt_usb_9axisimu_driver" required="true" output="screen">
    <param name="port" value="$(arg port)" />
    <param name="frame_id" value="$(arg frame_id)" />
  </node>

  <node pkg="imu_complementary_filter" type="complementary_filter_node" name="filter" >
    <param name="fixed_frame" value="map" />
    <param name="publish_tf" value="true" />
    <remap from="imu/data" to="/imu_raw" />
  </node>
</launch>
